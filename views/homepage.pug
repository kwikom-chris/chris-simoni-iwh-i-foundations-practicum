doctype html
html 
    head 
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body    
        table
          tr
            th Name
            th Type
            th Age
          each pet in data
                tr
                  td #{pet.properties.name}
                  td #{pet.properties.pet_type}
                  td #{pet.properties.pet_age}
